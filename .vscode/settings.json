// 将设置放入此文件中以覆盖默认值和用户设置。
{
  "editor.tabSize": 2,
  "emmet.syntaxProfiles": {
    "vue-html": "html",
    "vue": "html"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "html",
    "vue"
  ],
  "eslint.options": {
    "plugins": [
      "html"
    ]
  },
  "prettier.singleQuote": true,
  "prettier.semi": false
}